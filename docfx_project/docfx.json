{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "VestPocket.csproj"
          ],
          "src": "../VestPocket"
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "Vest Pocket",
      "_appLogo": "icon.png",
      "_appName": "Vest Pocket",
      "_disableContribution": true,
      "_disableTocFilter": true,
      "_disableBreadcrumb": true
    },
    "fileMetadata": {
      "redirect_url": {
        "index.md": "api/README.html"
      }
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "api/README.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}